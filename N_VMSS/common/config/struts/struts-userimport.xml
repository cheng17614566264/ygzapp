<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
    "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="userimport" extends="cfa" namespace="/">

		<!-- list user import -->
		<action name="viewUserImport" method="list" class="userImportAction">
			<result name="success">/page/userimport/viewUserImport.jsp</result>
		</action>

		<action name="viewUserImportOnOrgChange" method="onOrgChange"
			class="userImportAction">
			<result name="success">/page/userimport/viewUserImport.jsp</result>
		</action>

		<action name="viewUserImportSaveAndRun" method="saveAndRun"
			class="userImportAction">
			<result name="success">/page/userimport/viewUserImport.jsp</result>
		</action>
	</package>
</struts>